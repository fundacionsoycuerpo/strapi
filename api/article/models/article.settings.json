{
  "connection": "default",
  "collectionName": "Articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "published_at": {
      "type": "date"
    },
    "content": {
      "type": "richtext"
    },
    "gallery_url": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "tags": {
      "collection": "tag",
      "via": "articles",
      "dominant": true
    }
  }
}
